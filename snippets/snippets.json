{
	"addi": {
		"prefix": "addi",
		"body": "addi\t${1:\\$t0}, ${2:\\$t1}, ${3:0}\t\t\t# $1 = $2 + $3\n",
		"description": "add immediate",
		"scope": "source.mips"
	},
	"add": {
		"prefix": "add",
		"body": "add\t\t${1:\\$t0}, ${2:\\$t1}, ${3:\\$t2}\t\t# $1 = $2 + $3\n",
		"description": "add",
		"scope": "source.mips"
	},
	"and": {
		"prefix": "and",
		"body": "and\t\t${1:\\$t0}, ${2:\\$t1}, ${3:\\$t2}\t\t# $1 = $2 & $3\n",
		"description": "and",
		"scope": "source.mips"
	},
	"andi": {
		"prefix": "andi",
		"body": "andi\t${1:\\$t0}, ${2:\\$t1}, ${3:0}\t\t\t# $1 = $2 & $3\n",
		"description": "and immediate",
		"scope": "source.mips"
	},
	"beq": {
		"prefix": "beq",
		"body": "beq\t\t${1:\\$t0}, ${2:\\$t1}, ${3:target}\t# if $1 == $2 then $3\n",
		"description": "branch if equal",
		"scope": "source.mips"
	},
	"bge": {
		"prefix": "bge",
		"body": "bge\t\t${1:\\$t0}, ${2:\\$t1}, ${3:target}\t# if $1 >= $2 then $3\n",
		"description": "branch if greater then or equal",
		"scope": "source.mips"
	},
	"bgt": {
		"prefix": "bgt",
		"body": "bgt\t\t${1:\\$t0}, ${2:\\$t1}, ${3:target}\t# if $1 > $2 then $3\n",
		"description": "branch if greater then",
		"scope": "source.mips"
	},
	"ble": {
		"prefix": "ble",
		"body": "ble\t\t${1:\\$t0}, ${2:\\$t1}, ${3:target}\t# if $1 <= $2 then $3\n",
		"description": "branch if less then or equal",
		"scope": "source.mips"
	},
	"blt": {
		"prefix": "blt",
		"body": "blt\t\t${1:\\$t0}, ${2:\\$t1}, ${3:target}\t# if $1 < $2 then $3\n",
		"description": "branch if less then",
		"scope": "source.mips"
	},
	"bne": {
		"prefix": "bne",
		"body": "bne\t\t${1:\\$t0}, ${2:\\$t1}, ${3:target}\t# if $1 != $2 then $3\n",
		"description": "branch if not equal",
		"scope": "source.mips"
	},
	"b": {
		"prefix": "b",
		"body": "b\t\t${1:target}\t\t\t# branch to $1\n",
		"description": "branch uncodnitional",
		"scope": "source.mips"
	},
	"div": {
		"prefix": "div",
		"body": "div\t\t${1:\\$t0}, ${2:\\$t1}\t\t\t# $1 / $2\nmflo\t${3:\\$t2}\t\t\t\t\t# $3 = floor($1 / $2) \nmfhi\t${4:\\$t3}\t\t\t\t\t# $4 = $1 mod $2 \n",
		"description": "divide",
		"scope": "source.mips"
	},
	"jal": {
		"prefix": "jal",
		"body": "jal\t\t${1:target}\t\t\t\t# jump to $1 and save position to \\$ra\n",
		"description": "jump and link",
		"scope": "source.mips"
	},
	"jr": {
		"prefix": "jr",
		"body": "jr\t\t${1:\\$ra}\t\t\t\t\t# jump to $1\n",
		"description": "jump register",
		"scope": "source.mips"
	},
	"j": {
		"prefix": "j",
		"body": "j\t\t${1:target}\t\t\t\t# jump to $1\n",
		"description": "jump",
		"scope": "source.mips"
	},
	"la": {
		"prefix": "la",
		"body": "la\t\t${1:\\$t1}, ${2:var1}\t\t# \n",
		"description": "load address",
		"scope": "source.mips"
	},
	"lb": {
		"prefix": "lb",
		"body": "lb\t\t${1:\\$t1}, ${2:${3:0}(${4:\\$s1})}\t\t# \n",
		"description": "load byte",
		"scope": "source.mips"
	},
	"li": {
		"prefix": "li",
		"body": "li\t\t${1:\\$t1}, $2\t\t# $1 = $2\n",
		"description": "load immediate",
		"scope": "source.mips"
	},
	"lw": {
		"prefix": "lw",
		"body": "lw\t\t${1:\\$t1}, ${2:${3:0}(${4:\\$s1})}\t\t# \n",
		"description": "load word",
		"scope": "source.mips"
	},
	"move": {
		"prefix": "move",
		"body": "move \t${1:\\$t0}, ${2:\\$t1}\t\t# $1 = $2",
		"description": "move",
		"scope": "source.mips"
	},
	"mult": {
		"prefix": "mult",
		"body": "mult\t${1:\\$t0}, ${2:\\$t1}\t\t\t# $1 * $2 = Hi and Lo registers\nmflo\t${3:\\$t2}\t\t\t\t\t# copy Lo to $3\n",
		"description": "multiply",
		"scope": "source.mips"
	},
	"nor": {
		"prefix": "nor",
		"body": "nor\t\t${1:\\$t0}, ${2:\\$t1}, ${3:\\$t2}\t\t# $1 = ~($2 | $3)\n",
		"description": "nor",
		"scope": "source.mips"
	},
	"or": {
		"prefix": "or",
		"body": "or\t\t${1:\\$t0}, ${2:\\$t1}, ${3:\\$t2}\t\t# $1 = $2 | $3\n",
		"description": "or",
		"scope": "source.mips"
	},
	"ori": {
		"prefix": "ori",
		"body": "ori\t\t${1:\\$t0}, ${2:\\$t1}, ${3:0}\t\t\t# $1 = $2 | $3\n",
		"description": "or immediate",
		"scope": "source.mips"
	},
	"printInt": {
		"prefix": "print",
		"body": "li\t\t\\$v0, 1\t\t# system call #1 - print int\nla\t\t\\$a0, ${1:variable}\nsyscall\t\t\t\t# execute",
		"description": "print int",
		"scope": "source.mips"
	},
	"printFloat": {
		"prefix": "print",
		"body": "li\t\t\\$v0, 2\t\t# system call #2 - print float\nla\t\t\\$a0, ${1:variable}\nsyscall\t\t\t\t# execute",
		"description": "print float",
		"scope": "source.mips"
	},
	"printDouble": {
		"prefix": "print",
		"body": "li\t\t\\$v0, 3\t\t# system call #3 - print double\nla\t\t\\$a0, ${1:variable}\nsyscall\t\t\t\t# execute",
		"description": "print double",
		"scope": "source.mips"
	},
	"printString": {
		"prefix": "print",
		"body": "li\t\t\\$v0, 4\t\t# system call #4 - print string\nla\t\t\\$a0, ${1:string}\nsyscall\t\t\t\t# execute",
		"description": "print string",
		"scope": "source.mips"
	},
	"sb": {
		"prefix": "sb",
		"body": "sb\t\t${1:\\$t1}, ${2:${3:0}(${4:\\$s1})}\t\t# \n",
		"description": "store byte",
		"scope": "source.mips"
	},
	"sll": {
		"prefix": "sll",
		"body": "sll\t\t${1:\\$t0}, ${2:\\$t1}, ${3:0}\t\t\t# $1 = $2 << $3\n",
		"description": "shift left",
		"scope": "source.mips"
	},
	"slt": {
		"prefix": "slt",
		"body": "slt\t\t${1:\\$t0}, ${2:\\$s0}, ${3:\\$s1}\t\t# $1 = ($2 < $3) ? 1 : 0\n",
		"description": "set on less than",
		"scope": "source.mips"
	},
	"slti": {
		"prefix": "slti",
		"body": "slti\t${1:\\$t0}, ${2:\\$s0}, ${3:0}\t\t\t# $1 = ($2 < $3) ? 1 : 0\n",
		"description": "set on less than immediate",
		"scope": "source.mips"
	},
	"srl": {
		"prefix": "srl",
		"body": "srl\t\t${1:\\$t0}, ${2:\\$t1}, ${3:0}\t\t\t# $1 = $2 >> $3\n",
		"description": "shift right",
		"scope": "source.mips"
	},
	"sw": {
		"prefix": "sw",
		"body": "sw\t\t${1:\\$t1}, ${2:${3:0}(${4:\\$s1})}\t\t# \n",
		"description": "store word",
		"scope": "source.mips"
	},
	"string": {
		"prefix": "string",
		"body": "${1:string}: .asciiz \"$2\"",
		"description": "string var",
		"scope": "source.mips"
	},
	"sub": {
		"prefix": "sub",
		"body": "sub\t\t${1:\\$t0}, ${2:\\$t1}, ${3:\\$t2}\t\t# $1 = $2 - $3\n",
		"description": "subtract",
		"scope": "source.mips"
	},
	"subi": {
		"prefix": "subi",
		"body": "subi\t${1:\\$t0}, ${2:\\$t1}, ${3:0}\t\t\t# $1 = $2 - $3\n",
		"description": "subtract immediate",
		"scope": "source.mips"
	},
	"var": {
		"prefix": "var",
		"body": "${1:var}: .${2:word} $3",
		"description": "variable",
		"scope": "source.mips"
	},
	"xor": {
		"prefix": "xor",
		"body": "xor\t\t${1:\\$t0}, ${2:\\$t1}, ${3:\\$t2}\t\t# $1 = $2 ^ $3\n",
		"description": "xor",
		"scope": "source.mips"
	},
	"xori": {
		"prefix": "xori",
		"body": "xori\t${1:\\$t0}, ${2:\\$t1}, ${3:0}\t\t\t# $1 = $2 ^ $3\n",
		"description": "xor immediate",
		"scope": "source.mips"
	}
}